// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Device {
  id Int @id @default(autoincrement())
  clientType String
  clientName String?
  osName String
  osVersion String?
  deviceTrype String
  deviceBrand String?
  data Json
  createdAt  DateTime   @default(now())
}
